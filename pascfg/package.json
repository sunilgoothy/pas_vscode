{
    "name": "pascfg",
    "displayName": "PASCFG",
    "description": "PAS Language Syntax and Snippets",
    "icon": "tmeic.ico",
    "version": "0.1.1",
    "publisher": "sunilgoothy",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cfg",
                "aliases": [
                    "PASCFG",
                    "cfg"
                ],
                "extensions": [
                    ".cfg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cfg",
                "scopeName": "source.cfg",
                "path": "./syntaxes/cfg.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cfg",
                "path": "./snippets/cfg.snippets.json"
            }
        ]
    }
}